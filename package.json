{
    "name": "info-scraper",
    "version": "1.0.0",
    "description": "A full project to scrap data from the web",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "build": "rm -rf ./dist && tsc --project tsconfig.json && tsc-alias",
        "start": "tsx ./src/index.ts",
        "dev": "tsx watch ./src/index.ts",
        "test": "jest",
        "image:build": "npm run build && docker build -t infoscraper:1.0.0 .",
        "migrate:new": "npx sequelize-cli migration:generate --name $_name",
        "migrate:do": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo"
    },
    "author": "Henrique Manduca",
    "license": "ISC",
    "engines": {
        "node": "18.17"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.9",
        "@types/node-schedule": "^2.1.0",
        "@types/numeral": "^2.0.2",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.2",
        "prettier": "^3.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.7",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "class-transformer": "^0.5.1",
        "date-fns-tz": "^2.0.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "node-cache": "^5.1.2",
        "node-schedule": "^2.1.1",
        "numeral": "^2.0.6",
        "pg": "^8.11.1",
        "puppeteer": "^20.8.3",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.21.5",
        "sequelize-cli": "^6.6.1",
        "sequelize-typescript": "^2.1.3",
        "uuid": "^9.0.0"
    }
}